# API Configuration
VITE_API_URL=http://localhost:3001/api/v1
VITE_APP_NAME=CounselFlow Ultimate 5.1
VITE_APP_VERSION=5.1.0

# AI Provider API Keys (Replace with your actual keys)
VITE_OPENAI_API_KEY=YOUR_OPENAI_API_KEY_HERE
VITE_DEEPSEEK_API_KEY=YOUR_DEEPSEEK_API_KEY_HERE
VITE_OLLAMA_URL=http://localhost:11434
VITE_GEMINI_API_KEY=YOUR_GEMINI_API_KEY_HERE
VITE_CLAUDE_API_KEY=YOUR_CLAUDE_API_KEY_HERE

# Environment
VITE_ENVIRONMENT=development

# Feature Flags
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true

# Session Configuration
VITE_SESSION_TIMEOUT_HOURS=24
VITE_REMEMBER_ME_DAYS=30

# Logging Configuration
VITE_LOG_LEVEL=debug
VITE_ENABLE_REMOTE_LOGGING=false
VITE_REMOTE_LOG_ENDPOINT=/api/logs

# Security
VITE_ENABLE_CSP=true
VITE_SECURE_COOKIES=true

# Demo Mode
VITE_DEMO_MODE=true
VITE_DEMO_AUTO_LOGIN=false